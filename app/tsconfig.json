{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "module": "ES2020",
        "moduleResolution": "node",
        "target": "es5",
        "jsx": "react",
        "allowJs": false,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@styles/*": [
                "src/assets/styles/*"
            ],
            "@images/*": [
                "src/assets/images/*"
            ],
        }
    },
}